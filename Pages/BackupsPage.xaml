    <Border CornerRadius="15" Background="{DynamicResource MaterialDesignPaper}">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="101*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="150" />
                <ColumnDefinition Width="650"/>
            </Grid.ColumnDefinitions>
            <!-- Nav -->
            <StackPanel Grid.Column="0" Grid.Row="0"
                        Orientation="Vertical">
                <Image HorizontalAlignment="Left" Source="../Images/Logo.png" Height="57" Width="180" Margin="-10  0 0 0"/>
                <Button HorizontalAlignment="Left" Style="{StaticResource SeamlessBtn}" Margin="0 15 0 0">
                    <StackPanel Orientation="Horizontal">
                        <Path Width="30" Data="{StaticResource VirusGeo}" Fill="White" Stretch="Uniform" Margin="0 0 10 0"/>
                        <TextBlock>Anti-Virus</TextBlock>
                    </StackPanel>
                </Button>
                <Button HorizontalAlignment="Left" Style="{StaticResource SeamlessBtn}" Margin="0 15 0 0">
                    <StackPanel Orientation="Horizontal">
                        <Path Width="30" Data="{StaticResource PassShieldGeo}" Fill="White" Stretch="Uniform" Margin="0 0 10 0"/>
                        <TextBlock>Passwords</TextBlock>
                    </StackPanel>
                </Button>
                <Button HorizontalAlignment="Left" Style="{StaticResource SeamlessBtn}" Margin="0 15 0 0">
                    <StackPanel Orientation="Horizontal">
                        <Path Width="30" Data="{StaticResource UploadGeo}" Fill="White" Stretch="Uniform" Margin="0 0 10 0 "/>
                        <TextBlock>Backups</TextBlock>
                    </StackPanel>
                </Button>
                <Button HorizontalAlignment="Left" Style="{StaticResource SeamlessBtn}" Margin="0 15 0 0">
                    <StackPanel Orientation="Horizontal">
                        <Path Width="30" Data="{StaticResource BeatGeo}"
                              Fill="White" Stretch="Uniform" Margin="0 0 10 0 "/>
                        <TextBlock>Analysis</TextBlock>
                    </StackPanel>
                </Button>
                <Button HorizontalAlignment="Left" Style="{StaticResource SeamlessBtn}" Margin="0 15 0 0">
                    <StackPanel Orientation="Horizontal">
                        <Path Width="30" Data="{StaticResource UserGeo}"
                              Fill="White" Stretch="Uniform" Margin="0 0 10 0 "/>
                        <TextBlock>]FILLER[</TextBlock>
                    </StackPanel>
                </Button>
                <StackPanel Orientation="Horizontal"
                            Margin="0 110 0 0">
                    <Button Style="{StaticResource SeamlessBtn}"
                        HorizontalAlignment="Left" VerticalAlignment="Bottom"
                        Margin="0 0 -1  0">
                    <Path x:Name="ThemeSwitch" Data="{StaticResource SunGeo}"
                          Fill="White"
                          Stretch="Uniform"/>
                    </Button>
                    <Button Style="{StaticResource SeamlessBtn}"
                            HorizontalAlignment="Left" VerticalAlignment="Bottom"
                            Margin="0 0 -1  0">
                        <Path Data="{StaticResource MailGeo}"
                              Fill="White"
                              Stretch="Uniform"/>
                    </Button>
                    <Button Style="{StaticResource SeamlessBtn}"
                            HorizontalAlignment="Left" VerticalAlignment="Bottom"
                            Margin="0 0 -10  0">
                        <Path Data="{StaticResource QuestionGeo}"
                              Fill="White"
                              Stretch="Uniform"/>
                    </Button>
                </StackPanel>
            </StackPanel>

            <StackPanel Grid.Column="1">
                
            </StackPanel>
        </Grid>
    </Border>
</Page>
